	"BuscaBinariaCPP": {
    "prefix": "buscaBin",
    "body": [
      "#include <iostream>",
      "int buscaBinaria(int vetor[], int tamanho, int valor)",
      "{",
      "  int inicio = 0, fim = tamanho - 1, meio;",
      "  while (inicio <= fim)",
      "  {",
      "    meio = (inicio + fim) / 2;",
      "    if (vetor[meio] == valor)",
      "      return meio; // valor encontrado",
      "    else if (vetor[meio] < valor)",
      "      inicio = meio + 1; // busca na metade superior",
      "    else",
      "      fim = meio - 1; // busca na metade inferior",
      "  }",
      "  return -1; // não encontrado",
      "}",
      "",
      "int main()",
      "{",
      "  int vetor[] = {3, 6, 9, 12, 15, 18, 21};",
      "  int valor = 15;",
      "  int pos = buscaBinaria(vetor, 7, valor);",
      "  if (pos != -1)",
      "    std::cout << \"Valor: \" << valor << \" encontrado na posicao : \" << pos << \".\\\\n\";",
      "  else",
      "    std::cout << \"Valor nao encontrado.\\\\n\";",
      "  return 0;",
      "}"
    ],
    "description": "Implementação de busca binária em C++"
  },
	"BubbleSortCPP": {
  "prefix": "bubbleSort",
  "body": [
    "#include <iostream>",
    "void bubbleSort(int arr[], int n)",
    "{",
    "  for (int i = 0; i < n - 1; i++)",
    "  {",
    "    for (int j = 0; j < n - i - 1; j++)",
    "    {",
    "      if (arr[j] > arr[j + 1])",
    "      {",
    "        int temp = arr[j];",
    "        arr[j] = arr[j + 1];",
    "        arr[j + 1] = temp;",
    "      }",
    "    }",
    "  }",
    "}",
    "int main()",
    "{",
    "  int arr[] = {5, 1, 4, 2, 8};",
    "  int tam = 5;",
    "  bubbleSort(arr, tam);",
    "  std::cout << \"Array ordenado: \";",
    "  for (int i = 0; i < tam; i++)",
    "    std::cout << arr[i] << \" \";",
    "  return 0;",
    "}"
  ],
  "description": "Implementação do algoritmo Bubble Sort em C++"
	},
	"SelectionSortCPP": {
  "prefix": "selectionSort",
  "body": [
    "#include <iostream>",
    "void selectionSort(int arr[], int n)",
    "{",
    "  for (int i = 0; i < n - 1; i++)",
    "  {",
    "    int min_idx = i;",
    "    for (int j = i + 1; j < n; j++)",
    "    {",
    "      if (arr[j] < arr[min_idx])",
    "        min_idx = j;",
    "    }",
    "    int temp = arr[min_idx];",
    "    arr[min_idx] = arr[i];",
    "    arr[i] = temp;",
    "  }",
    "}",
    "int main()",
    "{",
    "  int arr[] = {64, 25, 12, 22, 11};",
    "  int tam = 5;",
    "  selectionSort(arr, tam);",
    "  std::cout << \"Array ordenado: \";",
    "  for (int i = 0; i < tam; i++)",
    "    std::cout << arr[i] << \" \";",
    "  return 0;",
    "}"
  ],
  "description": "Implementação do Selection Sort em C++"
	},
	"InsertionSortCPP": {
  "prefix": "insertionSort",
  "body": [
    "#include <iostream>",
    "void insertionSort(int arr[], int n)",
    "{",
    "  for (int i = 1; i < n; i++)",
    "  {",
    "    int key = arr[i];",
    "    int j = i - 1;",
    "    while (j >= 0 && arr[j] > key)",
    "    {",
    "      arr[j + 1] = arr[j];",
    "      j--;",
    "    }",
    "    arr[j + 1] = key;",
    "  }",
    "}",
    "int main()",
    "{",
    "  int arr[] = {12, 11, 13, 5, 6};",
    "  int n = 5;",
    "  insertionSort(arr, n);",
    "  std::cout << \"Array ordenado: \";",
    "  for (int i = 0; i < n; i++)",
    "    std::cout << arr[i] << \" \";",
    "  return 0;",
    "}"
  ],
  "description": "Implementação do Insertion Sort em C++"
	}

